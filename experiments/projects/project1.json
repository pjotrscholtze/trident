{
    "name": "doing-something",
    "description": "example_project",
    "github_url": "https://github.com/pjotrscholtze/trident.git",
    "github_checkout": "master",
    "script": [
        "#!/bin/bash -e",
        "#SBATCH -t 1:00 -N 1 -n 8 --mem=1600M",
        "#SBATCH --output=$PROJECT_PATH/slurm_%j.out",
        "rm -rf $PROJECT_PATH/db",
        "time $PROJECT_PATH/trident/build/trident load -i $PROJECT_PATH/db -f /var/scratch/pse740/latest-lexemes.nt.gz",
        "du -h -d0 $PROJECT_PATH/db",
        "rm -rf $PROJECT_PATH/db"
    ]
}